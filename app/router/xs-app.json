{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/apikey/(.*)",
      "target": "/tokenizer/$1",
      "service": "html5-apps-repo-rt"
    },
    {
      "source": "^/appconfig/fioriSandboxConfig.json(.*)$",
      "target": "/app/appconfig/fioriSandboxConfig.json",
      "service": "html5-apps-repo-rt"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true
    }
  ]
}
